BAAA,CAAA;AACA,eAAA,CAAA;AACA;AACA;AAPA;AAtCA,CAAA","sourcesContent":["<template>\n  <div>\n    <div class=\"teams\" v-for=\"(team, index) in listTeams\" :key=\"index\">\n      <h3 class=\"name\">{{ team.name }}</h3>\n      <p class=\"balls\">{{ team.balls }} {{ ballsName(index) }}</p>\n    </div>\n\n    <aside>\n      <select v-model=\"chengeTeam\">\n        <option \n          v-for=\"(team, index) in listTeams\" \n          :key=\"index\"\n          :value=\"index\"\n        >\n        {{ team.name }}\n      </option>\n      </select>\n      <input type=\"text\" v-model=\"inputCount\" placeholder=\"Баллы\">\n      <button @click=\"addBalls()\">add</button>\n      <button @click=\"delBalls()\">del</button>\n    </aside>\n  </div>\n</template>\n\n<script>\nexport default {\n  data: function() {\n    return {\n      chengeTeam : 0,\n      inputCount : \"\",\n      listTeams: [\n        {\n          name : \"asd\",\n          balls : 0\n        },\n        {\n          name : \"asd\",\n          balls : 0\n        },\n        {\n          name : \"asd\",\n          balls : 0\n        },\n      ]\n    }\n  },\n  methods:{\n    ballsName : function(index) {\n      if (this.listTeams[index].balls < 10) return \"балла\";\n      else return \"баллов\"\n    },\n    addBalls : function() {\n      let team = this.chengeTeam;\n      this.listTeams[team].balls += this.count;\n    },\n    delBalls : function() {\n      let team = this.chengeTeam;\n      this.listTeams[team].balls -= this.count;\n    },\n    isNum : function(str) {\n      return /[0-9]/.test(str)\n    }\n  },\n  computed : {\n    count : function(){\n      if (/[0-9]/.test(this.inputCount)) return parseInt(this.inputCount)\n      else{\n        alert(\"Неправильное число\")\n        return 0;\n      }\n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n\n</style>\n"],"sourceRoot":"src/components"}]}